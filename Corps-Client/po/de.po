#: src/arghandler.cc:53 src/arghandler.cc:61 src/arghandler.cc:94
msgid "you have to specify exactly one game or one list"
msgstr "Sie müssen genau ein Spiel oder eine Liste angeben"

#: src/arghandler.cc:67 src/arghandler.cc:88
#, c-format
msgid "Usage: %s [OPTIONS]\n"
msgstr "Benutzung: %s [OPTIONEN]\n"

#: src/arghandler.cc:69
msgid ""
"Option summary:\n"
"\n"
" -g --game          - connect to specific game\n"
" -l --list          - choose game from list\n"
" -h --help          - print this text and exit\n"
" -v --version       - print version information and exit\n"
"\n"
"Report bugs to srittau@jroger.in-berlin.de\n"
msgstr ""
"Optionsüberblick:\n"
"\n"
" -g --game       - Mit einem speziellen Spiel verbinden.\n"
" -l --list       - Spiel aus Liste auswählen.\n"
" -h --help       - Anzeigen dieser Hilfe und beenden.\n"
" -v --version    - Ausgabe der Versionsinformation und beenden.\n"
"\n"
"Bitte melden Sie Fehler an srittau@jroger.in-berlin.de\n"

